.page-header
  .pull-left
    h1= t('backend')

.row#dashboards
  .col-md-6
    .panel
      .panel-body
        h2.has-divider.text-highlight= t('shark')
        ul.list-inline
          li
            == link_to t('shark'), consoles_sharks_path
          li
            == link_to "#{t('helpers.add')}#{t('shark')}", new_consoles_shark_path
  .col-md-6
    .panel
      .panel-body
        h2.has-divider.text-highlight= t('developer_profile')
        ul.list-inline
          li
            == link_to t('helpers.profile'), profiles_consoles_users_path
          li
            == link_to t('helpers.donate'), profiles_consoles_users_path(anchor: 'donate')
          li
            == link_to t('helpers.update_password'), profiles_consoles_users_path(anchor: 'password')
